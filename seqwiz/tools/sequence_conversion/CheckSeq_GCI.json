{
    "appinfo": {
        "name": "CheckSeq",
        "version": "1.0",
        "description": "Check parsing rule (for fasta/peff files) or sequence file (including sqpd, set and peplis)",
        "developer": "",
        "contact": "",
        "homepage": "",
        "license": "",
        "standard": ""
    },
    "gcipars": {
        "cuiapp": "CheckSeq.py",
        "atqctrl": "False",
        "cmdctrl": "True",
        "icon": "",
        "logo": "logo.png",
        "splash": "splash.png",
        "help": "",
        "wait": "",
        "fadeio":"True",
        "transparency":"240",
        "interpreter":"python",
        "sptime": "5",
        "spmaxsz": "[480,240]",
        "fontsz": "12",
        "logomaxsz": "[240,120]",
        "parasz": "[640,360]",
        "infowidth": "400",
        "descsz": "[480,60]",
        "titwrap": "180",
        "hpwrap": "240",
        "cmdsz": "[600,72]",
        "waitsz": "[480,96]"
    },
    "cuipars": {
        "si_1": {
            "title": "Input data file",
            "type": "file",
            "style": "single",
            "flag": "posl",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Input data file (absolute path)"
        },
        "si_2": {
            "title": "Data format",
            "type": "select",
            "style": "single",
            "flag": "-t",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "fasta|peff|sqpd|peplis|set",
            "default": "",
            "help": "Data format"
        },
        "si_3": {
            "title": "Parse rule",
            "type": "select",
            "style": "single",
            "flag": "-r",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "UniProt|Single|Vertical|Space|GI|PEFF",
            "default": "",
            "help": "Parse rule for fasta/peff file"
        },
        "si_4": {
            "title": "Number of entries",
            "type": "int",
            "style": "",
            "flag": "-f",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "5",
            "help": "Show first ? number of entries (default:5)"
        },
        "si_5": {
            "title": "Display sequence",
            "type": "check",
            "style": "",
            "flag": "-s",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Display sequence"
        }
    }
}