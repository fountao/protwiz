{
    "appinfo": {
        "name": "SepFinder",
        "version": "1.0",
        "description": "Prediction of small-ORFs (sORFs) and their encoded peptides (SEPs) based on RNA transcript sequences",
        "developer": "",
        "contact": "",
        "homepage": "",
        "license": "",
        "standard": ""
    },
    "gcipars": {
        "cuiapp": "SepFinder.py",
        "atqctrl": "False",
        "cmdctrl": "True",
        "icon": "",
        "logo": "logo.png",
        "splash": "splash.png",
        "help": "",
        "wait": "",
        "fadeio":"True",
        "transparency":"240",
        "interpreter":"python",
        "sptime": "5",
        "spmaxsz": "[480,240]",
        "fontsz": "12",
        "logomaxsz": "[240,120]",
        "parasz": "[640,360]",
        "infowidth": "400",
        "descsz": "[380,60]",
        "titwrap": "180",
        "hpwrap": "240",
        "cmdsz": "[600,72]",
        "waitsz": "[480,96]"
    },
    "cuipars": {
        "si_1": {
            "title": "Souce file",
            "type": "file",
            "style": "single",
            "flag": "posl",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Input fasta file (absoulte path) for transcripts"
        },
        "si_2": {
            "title": "Parse rule",
            "type": "select",
            "style": "single",
            "flag": "-r",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "Single|Vertical|Space|GI|PEFF",
            "default": "",
            "help": "Parse rule for fasta/peff file"
        },
        "si_3": {
            "title": "Label name",
            "type": "string",
            "style": "single",
            "flag": "-l",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Label name for output file"
        }, 
        "si_4": {
            "title": "Check mode",
            "type": "check",
            "style": "",
            "flag": "--ckseq",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Check mode to show the first parsed sequence only"
        },
        "si_5": {
            "title": "Initial codon level",
            "type": "select",
            "style": "single",
            "flag": "-i",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "C|N|A",
            "default": "",
            "help": "Level of initial codon usage: [C]anonical, [N]ear-cognate (with canonical) and [A]ll possible initial condons"
        },
        "si_6": {
            "title": "Enable ambiguous condons",
            "type": "check",
            "style": "",
            "flag": "-a",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Enable ambiguous DNA codons, will use X for ambiguous AA"
        },
        "si_7": {
            "title": "Minimal peptide length",
            "type": "int",
            "style": "",
            "flag": "--minlen",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "6",
            "help": "Minimal length for SEP, default: 6"
        },
        "si_8": {
            "title": "Maximal peptide length",
            "type": "int",
            "style": "",
            "flag": "--maxlen",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "200",
            "help": "Maximal length for sPEP, default: 200"
        }
    }
}