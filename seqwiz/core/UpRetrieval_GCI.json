{
    "appinfo": {
        "name": "UpRetrieval",
        "version": "1.0",
        "description": "Retrieval of species specific protein sequences and annotations from the UniProtKB database",
        "developer": "",
        "contact": "",
        "homepage": "",
        "license": "",
        "standard": ""
    },
    "gcipars": {
        "cuiapp": "UpRetrieval.py",
        "atqctrl": "False",
        "cmdctrl": "True",
        "icon": "",
        "logo": "logo.png",
        "splash": "splash.png",
        "help": "",
        "wait": "",
        "fadeio":"True",
        "transparency":"240",
        "interpreter":"python",
        "sptime": "5",
        "spmaxsz": "[480,240]",
        "fontsz": "12",
        "logomaxsz": "[240,120]",
        "parasz": "[640,360]",
        "infowidth": "400",
        "descsz": "[380,60]",
        "titwrap": "180",
        "hpwrap": "240",
        "cmdsz": "[600,72]",
        "waitsz": "[480,96]"
    },
    "cuipars": {
        "si_1": {
            "title": "Taxonomy ID",
            "type": "string",
            "style": "single",
            "flag": "-t",
            "requirement": "yes",
            "exclusive": "yes",
            "limitation": "",
            "default": "",
            "help": "Taxonomy ID for a specific species"
        },
        "si_2": {
            "title": "Show UniProtKB column names",
            "type": "check",
            "style": "",
            "flag": "-b",
            "requirement": "yes",
            "exclusive": "yes",
            "limitation": "",
            "default": "false",
            "help": "Browse UniProtKB column names for --addfts"
        },
        "si_3": {
            "title": "Search for a reference proteome",
            "type": "check",
            "style": "",
            "flag": "-r",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "To search for a reference proteome"
        },
        "si_4": {
            "title": "Select only curated proteins",
            "type": "check",
            "style": "",
            "flag": "-c",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Select only curated proteins (reviewed by Swiss-Prot)"
        },
        "si_5": {
            "title": "Remove fragment sequences",
            "type": "check",
            "style": "",
            "flag": "-f",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Remove fragment sequences"
        },
        "si_6": {
            "title": "Remove isoform sequences",
            "type": "check",
            "style": "",
            "flag": "-i",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Remove isoform sequences"
        },
        "si_7": {
            "title": "Additional column features",
            "type": "string",
            "style": "multiple",
            "flag": "--addfts",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Additional column features for sequence annotation, the build-in features are (mostly used for proteoform): id; entry name; protein names; genes; organism; proteome; created; last- modified; sequence-modified; reviewed; existence; fragment; comment(ALTERNATIVE PRODUCTS); feature(NATURAL VARIANT); feature(CHAIN); feature(CROSS LINK); feature(DISULFIDE BOND); feature(GLYCOSYLATION); feature(INITIATOR METHIONINE); feature(LIPIDATION); feature(MODIFIED RESIDUE); feature(PEPTIDE); feature(PROPEPTIDE); feature(SIGNAL); feature(TRANSIT); feature(SEQUENCE CONFLICT)."
        },
        "si_8": {
            "title": "Create SQPD database",
            "type": "check",
            "style": "",
            "flag": "-s",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Follow-up action: Create SQPD sequence database using UpConvert"
        }, 
        "si_9": {
            "title": "Sequence properties annotation",
            "type": "check",
            "style": "",
            "flag": "-a",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Follow-up action: Performing sequence annotation (calculating protein properties) using SeqAnnotate"
        }
    }
}