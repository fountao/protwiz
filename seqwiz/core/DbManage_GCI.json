{
    "appinfo": {
        "name": "DbManage",
        "version": "1.0",
        "description": "Database management: create and update sequence database from other source",
        "developer": "",
        "contact": "",
        "homepage": "",
        "license": "",
        "standard": ""
    },
    "gcipars": {
        "cuiapp": "DbManage.py",
        "atqctrl": "False",
        "cmdctrl": "True",
        "icon": "",
        "logo": "logo.png",
        "splash": "splash.png",
        "help": "",
        "wait": "",
        "fadeio":"True",
        "transparency":"240",
        "interpreter":"python",
        "sptime": "5",
        "spmaxsz": "[480,240]",
        "fontsz": "12",
        "logomaxsz": "[240,120]",
        "parasz": "[640,360]",
        "infowidth": "400",
        "descsz": "[380,60]",
        "titwrap": "180",
        "hpwrap": "240",
        "cmdsz": "[600,72]",
        "waitsz": "[480,96]"
    },
    "cuipars": {
        "si_1": {
            "title": "Database name",
            "type": "string",
            "style": "single",
            "flag": "posl",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Database name"
        },
        "si_2": {
            "title": "Database version",
            "type": "string",
            "style": "single",
            "flag": "posl",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Database version"
        },
        "si_3": {
            "title": "Management type",
            "type": "select",
            "style": "single",
            "flag": "-m",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "create|update",
            "default": "",
            "help": "Action type for management"
        },
        "si_4": {
            "title": "Input sequence file",
            "type": "file",
            "style": "single",
            "flag": "-i",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "FASTA file (*.fasta)|*.fasta|PEFF file (*.peff)|*.peff|All files (*.*)|*.*",
            "default": "",
            "help": "Input sequence file (absolute path)"
        },
        "si_5": {
            "title": "Sequence file format",
            "type": "select",
            "style": "single",
            "flag": "-f",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "fasta|peff",
            "default": "",
            "help": "Sequence file format"
        },
        "si_6": {
            "title": "Parse rule",
            "type": "select",
            "style": "single",
            "flag": "-r",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "UniProt|Single|Vertical|Space|GI|PEFF",
            "default": "",
            "help": "Parse rule for fasta file"
        },
        "si_7": {
            "title": "Make this as the current version",
            "type": "check",
            "style": "",
            "flag": "-c",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Make this as the current version"
        },
        "si_8": {
            "title": "Create SQPD database",
            "type": "check",
            "style": "",
            "flag": "-s",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Follow-up action: Create SQPD sequence database using UpConvert"
        }, 
        "si_9": {
            "title": "Sequence properties annotation",
            "type": "check",
            "style": "",
            "flag": "-a",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Follow-up action: Performing sequence annotation (calculating protein properties) using SeqAnnotate"
        }
    }
}
