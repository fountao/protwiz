            "requirement": "yes",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Input data file (absolute path)"
        },
        "si_2": {
            "title": "Conversion mode",
            "type": "select",
            "style": "single",
            "flag": "-t",
            "requirement": "yes",
            "exclusive": "no",
            "limitation": "fasta2sqpd|peff2sqpd|sqpd2fasta|set2fasta",
            "default": "",
            "help": "Conversion mode"
        },
        "si_3": {
            "title": "Parse rule",
            "type": "select",
            "style": "single",
            "flag": "-r",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "UniProt|Single|Vertical|Space|GI|PEFF",
            "default": "",
            "help": "Parse rule for fasta/peff file"
        },
        "si_4": {
            "title": "Output dir",
            "type": "folder",
            "style": "single",
            "flag": "-o",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Output dir"
        },
        "si_5": {
            "title": "Label name",
            "type": "string",
            "style": "single",
            "flag": "-l",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "",
            "help": "Label name for output file"
        }, 
        "si_6": {
            "title": "Pretty print fasta format",
            "type": "check",
            "style": "",
            "flag": "-p",
            "requirement": "no",
            "exclusive": "no",
            "limitation": "",
            "default": "false",
            "help": "Pretty print fasta format"
        }
    }
